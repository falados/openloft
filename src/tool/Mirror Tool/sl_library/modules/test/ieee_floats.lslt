<tests>
  <lsl-test>
    <name>Float to IEEE</name>
    <entryPoint>
      <fileName>Library.float2hex.lslm</fileName>
      <path>Float2IEEE</path>
    </entryPoint>
    <arguments>
      <lsl-float>16.240030</lsl-float>
    </arguments>
    <expectedReturn>
      <val class="lsl-integer">0x4181EB95</val>
    </expectedReturn>
    <expectations>
      <mode>nice</mode>
      <calls/>
    </expectations>
    <initialBindings/>
    <finalBindings/>
  </lsl-test>
  <lsl-test>
    <name>IEEE to Float</name>
    <entryPoint>
      <fileName>Library.float2hex.lslm</fileName>
      <path>IEEE2Float</path>
    </entryPoint>
    <arguments>
      <lsl-integer>0x4181EB95</lsl-integer>
    </arguments>
    <expectedReturn>
      <val class="lsl-float">16.240030</val>
    </expectedReturn>
    <expectations>
      <mode>nice</mode>
      <calls/>
    </expectations>
    <initialBindings/>
    <finalBindings/>
  </lsl-test>
  <lsl-test>
    <name>+Infinity</name>
    <entryPoint>
      <fileName>Library.float2hex.lslm</fileName>
      <path>IEEE2Float</path>
    </entryPoint>
    <arguments>
      <lsl-integer>0x7f800000</lsl-integer>
    </arguments>
    <expectedReturn/>
    <expectations>
      <mode>normal</mode>
      <calls/>
    </expectations>
    <initialBindings/>
    <finalBindings/>
  </lsl-test>
  <lsl-test>
    <name>-Infinity</name>
    <entryPoint>
      <fileName>Library.float2hex.lslm</fileName>
      <path>IEEE2Float</path>
    </entryPoint>
    <arguments>
      <lsl-integer>0xFF800000</lsl-integer>
    </arguments>
    <expectedReturn/>
    <expectations>
      <mode>nice</mode>
      <calls/>
    </expectations>
    <initialBindings/>
    <finalBindings/>
  </lsl-test>
  <lsl-test>
    <name>NaN</name>
    <entryPoint>
      <fileName>Library.float2hex.lslm</fileName>
      <path>IEEE2Float</path>
    </entryPoint>
    <arguments>
      <lsl-integer>0xffffffff</lsl-integer>
    </arguments>
    <expectedReturn/>
    <expectations>
      <mode>nice</mode>
      <calls/>
    </expectations>
    <initialBindings/>
    <finalBindings/>
  </lsl-test>
</tests>